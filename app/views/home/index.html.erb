<ul role="list" class="grid grid-cols-1 gap-x-4 gap-y-8 sm:grid-cols-4 sm:gap-x-6 lg:grid-cols-5 xl:gap-x-8">
    <% @properties.each do |property| %>
        <li class="relative">
          <div class="group block w-full aspect-w-10 aspect-h-7 rounded-lg bg-gray-100 focus-within:ring-2 focus-within:ring-offset-2">
            <img src="https://images.unsplash.com/photo-1603356033288-acfcb54801e6?ixlib=rb-1.2.1&ixid=MnwxMjA3fDB8MHxwaG90by1wYWdlfHx8fGVufDB8fHx8&auto=format&fit=crop&w=1180&q=80" alt="robot">
            <button type="button" class="absolute inset-0 focus:outline-none">
                <span class="sr-only">View details for image name</span>
            </button>
          </div>
          <p class="mt-2 block text-sm font-medium text-grey-900 truncate pointer-events-none"><%= property.city %>, <%= property.state %>, <%= property.country %></p>
          <p class="block text-sm font-medium text-gray-500 pointer-events-none">7,934 mi away</p>
          <p class="block text-sm font-medium text-gray-500 pointer-events-none">Jun 19 - 24</p>
          <p class="block text-sm font-medium text-gray-500 pointer-events-none">$172 per night</p>
        </li>
    <% end %>
</ul>
